	<Shape id="btnX" system="true" displayName="Blue Button" icon="Icons/btnBlue.png">
		<Properties>
			<PropertyGroup>
				<Property name="box" type="Dimension" p:lockHeight="true">95,30</Property>
				<Property name="topColor" displayName="Top Color" type="Color">#eeeff1</Property>
				<Property name="bottomColor" displayName="Bottom Color" type="Color">#dadce1</Property>
			</PropertyGroup>
			<PropertyGroup name="Button Properties">
				<!--Set button types-->
				<Property name="btnType" displayName="Button Type" type="Enum" p:enumValues="['default|Default','primary|Primary','purchase|Purchase']">default</Property>
				<Property name="state" displayName="State" type="Enum" p:enumValues="['normal|Normal', 'hover|Mouse Over', 'clicked|Clicked']">normal</Property>
				<Property name="position" displayName="Button Group Position" type="Enum" p:enumValues="['none|None','left|Left','middle|Middle','right|Right']">none</Property>
				<Property name="disabled" displayName="Disabled?" type="Bool">false</Property>
			</PropertyGroup>
			<PropertyGroup name="Text">
				<Property name="textContent" displayName="Button Label" type="PlainText" p:editInfo="({targetName: 'container', bound: new Bound(5, ($box.h/2)-15, $box.w-10, 30), font: $textFont, align: new Alignment(1, 1), multi: false})">Button</Property>
				<Property name="textFont" displayName="Font" type="Font"><E>$$defaultFont14</E></Property>
			</PropertyGroup>
			<PropertyGroup name="Additional CSS">
				<Property name="customStyle1" displayName="Background" type="PlainText"><![CDATA[]]></Property>
				<Property name="customStyle2" displayName="Text" type="PlainText"><![CDATA[]]></Property>
			</PropertyGroup>
		</Properties>
		<Behaviors>
            <For ref="divObject">
				<InnerText>$textContent.value</InnerText>
				<Attr>
                    <Arg>"style"</Arg>
                    <Arg>
						<!-- Standard CSS for mapping built-in palettes to CSS -->
						"font-family: " + $textFont.family + ";" +
						"font-size: " + $textFont.size + ";" +
						"font-style: " + $textFont.style + ";" +
						"font-weight: " + $textFont.weight + ";" +
						"text-decoration: " + $textFont.decor + ";" +
						
						<!-- Set your CSS behaviours -->
						"display: inline-block;" + 
						"border-width: 1px;" +
						"border-style: solid;" +

						"height: " + ($box.h-2) + "px;" +
						"width: " + ($box.w-2) + "px;" +
						"line-height: " + ($box.h-4) + "px;" +

						"text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.16);" +
						"text-align: center;" +
						"border-color:rgba(0,0,0,0.15) rgba(0,0,0,0.15) rgba(0,0,0,0.25);" +
						"text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.16);" +
						"box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.25);" +						
						($state.value == "clicked" ? "background-image: none; background-color: " + $bottomColor.toRGBString() + "; box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.15);" : "") +
						($state.value == "hover" ? "background-image: none; background-color: " + $bottomColor.toRGBString() + ";" : "") +
						($disabled.value ? "background-image: none; background-color: " + $bottomColor.toRGBString() + "; box-shadow: none; opacity: 0.65;" : "") +
						$customStyle1.value + 
						$customStyle2.value
					</Arg>
                </Attr>
				<CustomStyle>
					<Arg>"border-top-left-radius"</Arg>
					<Arg>$position.value == "none" || $position.value == "left" ? "5px" : "0px"</Arg>
				</CustomStyle>
				<CustomStyle>
					<Arg>"border-top-right-radius"</Arg>
					<Arg>$position.value == "none" || $position.value == "right" ? "5px" : "0px"</Arg>
				</CustomStyle>
				<CustomStyle>
					<Arg>"border-bottom-right-radius"</Arg>
					<Arg>$position.value == "none" || $position.value == "right" ? "5px" : "0px"</Arg>
				</CustomStyle>
				<CustomStyle>
					<Arg>"border-bottom-left-radius"</Arg>
					<Arg>$position.value == "none" || $position.value == "left" ? "5px" : "0px"</Arg>
				</CustomStyle>

				<CustomStyle>
					<Arg>"color"</Arg>
					<Arg>$btnType.value == "default" ? "#595959" : "#FFFFFF"</Arg>
				</CustomStyle>

				<!--Gradient Style for Primary Button-->
				<CustomStyle>
					<Arg>"background-image"</Arg>
					<Arg>$btnType.value == "purchase" ? "-moz-linear-gradient(top, #E7B433, #E79C2C 50%, #E59123 51%, #E97926)" : ""</Arg>
				</CustomStyle>
				<CustomStyle>
					<Arg>"background-image"</Arg>
					<Arg>$btnType.value == "purchase" ? "-moz-linear-gradient(top, #E7B433, #E79C2C 50%, #E59123 51%, #E97926)" : ""</Arg>
				</CustomStyle>
                
            </For>
            <For ref="container">
            	<Box>$box</Box>
            </For>
        </Behaviors>
