<Shapes xmlns="http://www.evolus.vn/Namespace/Pencil" 
		xmlns:p="http://www.evolus.vn/Namespace/Pencil" 
		xmlns:svg="http://www.w3.org/2000/svg" 
		xmlns:xlink="http://www.w3.org/1999/xlink" 
		id="hoiioiPhoneTemplate.Icons" 
		displayName="Hoiio iPhone Elements" 
		description="Common elements for iPhone Apps" 
		author="Hoiio" 
		url="http://www.evolus.vn/Pencil">
		
		<Script>
		<![CDATA[
			collection.customImage = {};
			collection.customImage['buttons'] = '';
		]]>	
	</Script>
	
	<Properties>
        <PropertyGroup name="Default Text Size">
            <Property name="defaultFontSmall" type="Font">Arial|normal|normal|13px</Property>
			<Property name="defaultFontRegular" type="Font">Arial|normal|normal|17px</Property>
			<Property name="defaultFontRegularBold" type="Font">Arial|bold|normal|17px</Property>
			<Property name="defaultFontLargeBold" type="Font">Arial|bold|normal|21px</Property>
        </PropertyGroup>
        <PropertyGroup name="Default Text Color">
			<Property name="defaultFontGrey" type="Color">#595959</Property>
			<Property name="defaultFontWhite" type="Color">#FFFFFF</Property>
			<Property name="defaultLinkColor" type="Color">#0081c7</Property>
        </PropertyGroup>

    </Properties>

	<Shape id="testIcon" displayName="Test Button" icon="Icons/contactIcon.png">
		<Properties>
			<PropertyGroup>
				<Property name="customStyle1" displayName="boxView" type="PlainText"><![CDATA[]]></Property>
				<Property name="type" displayName="User" type="Enum" p:enumValues="['buttons|Blue', 'grey|Grey', 'disabled|Disabled']">buttons</Property>
			</PropertyGroup>
		</Properties>
		
		<Behaviors>
			<For ref="image">
				<Attr>
					<Arg>"xlink:href"</Arg>
					<Arg>
						"data:image/png;base64," + collection.customImage[$type.value];
					</Arg>
					<Arg>PencilNamespaces.xlink</Arg>
				</Attr>
			</For>
		</Behaviors>
		
		<p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg" >
			<svg width="49px" height="34px" viewbox="0 0 49 34" >
				<image id="image" width="49" height="34" x="-20" y="0" preserveAspectRatio="xMinYMin slice" />
			</svg>
		</p:Content>
	</Shape>

	<Shape id="BtnBlue" displayName="Test Image" icon="Icons/btnBlue.png">
		<Properties>
			<PropertyGroup name="Properties">
				<Property name="textContent" displayName="Content" type="PlainText" p:editInfo="({targetName: 'container', bound: new Bound(5, ($box.h/2)-15, $box.w-10, 30), font: $textFont, align: new Alignment(1, 1), multi: false})">Button</Property>
				<Property name="type" displayName="State" type="Enum" p:enumValues="['blue|Blue', 'grey|Grey', 'inbetween|In Between']">buttons</Property>
			</PropertyGroup>
			<PropertyGroup name="Text">
				<Property name="textFont" displayName="Font" type="Font"><E>$$defaultFontRegularBold</E></Property>
				<Property name="textColor" displayName="Color" type="Color"><E>$$defaultFontWhite</E></Property>
			</PropertyGroup>
			<PropertyGroup name="Additional CSS">
				<Property name="customStyle1" displayName="Background" type="PlainText"><![CDATA[]]></Property>
				<Property name="customStyle2" displayName="Text" type="PlainText"><![CDATA[]]></Property>
			</PropertyGroup>
		</Properties>
		
		<Behaviors>
            <For ref="divObject">
                <Attr>
                    <Arg>"style"</Arg>
                    <Arg>
						<!-- Set your CSS behaviours -->
						"display: inline-block;" + 
						"width: 49px;" +
						"height: 34px;" +
						"background-image: url(data:image/png;base64," + collection.customImage['buttons'] + ");" +
						($type.value == 'grey' ? "background-position: -49px 0px;" : "") +
						($type.value == 'inbetween' ? "background-position: -24px 0px;" : "") +
						$customStyle1.value + 
						$customStyle2.value
					</Arg>
                </Attr>
            </For>
            <For ref="container">
                <Box>new Dimension(49, 34);</Box>
            </For>
        </Behaviors>
		
		<p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
            <foreignObject id="container" height="34px" width="49px">
                <div xmlns="http://www.w3.org/1999/xhtml" id="divObject"></div>
            </foreignObject>
        </p:Content>
		
	</Shape>
	
	<Shape id="headerDefault" displayName="Header Bar" icon="Icons/headerDefault.png">
		<Properties>
			<PropertyGroup>
				<Property name="box" type="Dimension" >320,44</Property>
			</PropertyGroup>
			<PropertyGroup name="Text">
				<Property name="textContent" displayName="Content" type="PlainText" p:editInfo="({targetName: 'container', bound: new Bound(5, ($box.h/2)-15, $box.w-10, 30), font: $textFont, align: new Alignment(1, 1), multi: false})">Header</Property>
				<Property name="textFont" displayName="Font" type="Font"><E>$$defaultFontRegularBold</E></Property>
				<Property name="textColor" displayName="Color" type="Color"><E>$$defaultFontWhite</E></Property>
			</PropertyGroup>
			<PropertyGroup name="Additional CSS">
				<Property name="customStyle1" displayName="Background" type="PlainText"><![CDATA[]]></Property>
				<Property name="customStyle2" displayName="Inner Shadow" type="PlainText"><![CDATA[]]></Property>
			</PropertyGroup>
		</Properties>
		
		<Behaviors>
            <For ref="divObject">
				<InnerText>$textContent.value</InnerText>
                <Attr>
                    <Arg>"style"</Arg>
                    <Arg>
						<!-- Standard CSS for mapping built-in palettes to CSS -->
						"font-family: " + $textFont.family + ";" +
						"font-size: " + $textFont.size + ";" +
						"font-style: " + $textFont.style + ";" +
						"font-weight: " + $textFont.weight + ";" +
						"text-decoration: " + $textFont.decor + ";" +
						"color: " + $textColor.toRGBString() + ";" +
						
						<!-- Set your CSS behaviours -->
						"display: inline-block;" + 
						"width: " + ($box.w) + "px;" +
						"height: " + ($box.h-1) + "px;" +
						"line-height: " + ($box.h-2-1) + "px;" +
						"border-bottom: 1px solid #20546f;" +
						"text-shadow: 0 1px 0 rgba(0, 0, 0, 0.16);" +
						"text-align: center;" +
						"background-image: -moz-linear-gradient(top, #2d9bd6 0%, #1c90d0 33%, #0986ca 66%, #0081c7 100%);" +
						"color:#ffffff;" +
						"box-shadow: 0 1px 2px rgba(0, 0, 0, 0.25), inset 0 1px 0 rgba(255,255,255,0.15);" +						
						$customStyle1.value 
					</Arg>
                </Attr>
            </For>
			<For ref="bottomInnerShadow">
				<Attr>
					<Arg>"style"</Arg>
					<Arg>
						"display: block;" +
						"background-color: #0070ad;" +
						"height: 2px;" +
						"width: " + ($box.w) + "px;" +
						"position: absolute;" +
						"bottom:4px;" +
						$customStyle2.value
					</Arg>
				</Attr>
			</For>
            <For ref="container">
                <Box>new Dimension($box.w, $box.h+3);</Box>
            </For>
        </Behaviors>
		
		<p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
            <foreignObject id="container">
                <div xmlns="http://www.w3.org/1999/xhtml" id="divObject"></div>
                <div xmlns="http://www.w3.org/1999/xhtml" id="bottomInnerShadow"></div>
            </foreignObject>
        </p:Content>
		
	</Shape>
	
	<Shape id="headerCustom" displayName="Header Bar Custom" icon="Icons/headerDefault.png">
		<Properties>
			<PropertyGroup>
				<Property name="box" type="Dimension" >320,44</Property>
			</PropertyGroup>
			<PropertyGroup name="Text">
				<Property name="textContent" displayName="Content" type="PlainText" p:editInfo="({targetName: 'container', bound: new Bound(5, ($box.h/2)-15, $box.w-10, 30), font: $textFont, align: new Alignment(1, 1), multi: false})">Header</Property>
				<Property name="textFont" displayName="Font" type="Font"><E>$$defaultFontRegularBold</E></Property>
				<Property name="textColor" displayName="Color" type="Color"><E>$$defaultFontWhite</E></Property>
			</PropertyGroup>
			<PropertyGroup name="Background">
				<Property name="themeColor" type="Color" displayName="Theme Color">#0081C7ff</Property>
				<Property name="shading1" displayName="1st Shading" type="PlainText">0.06</Property>
				<Property name="shading2" displayName="2st Shading" type="PlainText">0.18</Property>
			</PropertyGroup>

			<PropertyGroup name="Additional CSS">
				<Property name="customStyle1" displayName="Background" type="PlainText"><![CDATA[]]></Property>
				<Property name="customStyle2" displayName="Inner Shadow" type="PlainText"><![CDATA[]]></Property>
			</PropertyGroup>
		</Properties>
		
		<Behaviors>
            <For ref="divObject">
				<InnerText>$textContent.value</InnerText>
                <Attr>
                    <Arg>"style"</Arg>
                    <Arg>
						<!-- Standard CSS for mapping built-in palettes to CSS -->
						"font-family: " + $textFont.family + ";" +
						"font-size: " + $textFont.size + ";" +
						"font-style: " + $textFont.style + ";" +
						"font-weight: " + $textFont.weight + ";" +
						"text-decoration: " + $textFont.decor + ";" +
						"color: " + $textColor.toRGBString() + ";" +
						
						<!-- Set your CSS behaviours -->
						"display: inline-block;" + 
						"width: " + ($box.w) + "px;" +
						"height: " + ($box.h-1) + "px;" +
						"line-height: " + ($box.h-2-1) + "px;" +
						"text-shadow: 0 1px 0 rgba(0, 0, 0, 0.16);" +
						"text-align: center;" +
						"box-shadow: 0 1px 2px rgba(0, 0, 0, 0.25), inset 0 1px 0 rgba(255,255,255,0.15);" +
						"background-image: -moz-linear-gradient(top, " + $themeColor.toRGBString() + " 0%, " + $themeColor.shaded($shading1.value).toRGBString() + " 50%, " + $themeColor.shaded($shading2.value).toRGBString() + ");" +
						"border-bottom: 1px solid " + $themeColor.shaded(0.55).toRGBString() + ";" +
						$customStyle1.value 
					</Arg>
                </Attr>
            </For>
			<For ref="bottomInnerShadow">
				<Attr>
					<Arg>"style"</Arg>
					<Arg>
						"display: block;" +
						"background-color: rgba(0,0,0,0.15);" +
						"height: 2px;" +
						"width: " + ($box.w) + "px;" +
						"position: absolute;" +
						"bottom:4px;" +
						$customStyle2.value
					</Arg>
				</Attr>
			</For>
            <For ref="container">
                <Box>new Dimension($box.w, $box.h+3);</Box>
            </For>
        </Behaviors>
		
		<p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
            <foreignObject id="container">
                <div xmlns="http://www.w3.org/1999/xhtml" id="divObject"></div>
                <div xmlns="http://www.w3.org/1999/xhtml" id="bottomInnerShadow"></div>
            </foreignObject>
        </p:Content>
		
	</Shape>

</Shapes>
