    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:cc="http://web.resource.org/cc/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:inkscape="http://www.inkscape.org/namespaces/inkscape"
    xmlns:sodipodi="http://sodipodi.sourceforge.net/DTD/sodipodi-0.dtd"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    id="testShape"
    displayName="Test Shape"
    description="Collection of basic items that are regularly used in web designs."
    author="Henry Wahyu"
    url="http://">

	<Properties>
        <PropertyGroup name="Font Sizes">
            <Property name="defaultFont11" type="Font">Segoe UI|normal|normal|11px</Property>
            <Property name="defaultFont12" type="Font">Segoe UI|normal|normal|12px</Property>
			<Property name="defaultFont14" type="Font">Segoe UI|normal|normal|14px</Property>
			<Property name="defaultFont14Bold" type="Font">Segoe UI|bold|normal|14px</Property>
			<Property name="defaultFont16Bold" type="Font">Segoe UI|bold|normal|16px</Property>
			<Property name="defaultFont18Bold" type="Font">Segoe UI|bold|normal|18px</Property>
			<Property name="heading3" type="Font">Segoe UI|bold|normal|22px</Property>
		</PropertyGroup>
		<PropertyGroup name="Font Color">
			<Property name="defaultFontColor" type="Color">#595959</Property>
			<Property name="defaultLinkColor" type="Color">#0081c7</Property>
			<Property name="defaultDisabledTextColor" type="Color">#999999</Property>
        </PropertyGroup>
    </Properties>

	<Shape id="horizontalRuler" displayName="Horizontal Ruler" icon="Icons/sectionHeader.png">
		<Properties>
			<PropertyGroup name="Properties">
				<Property name="hasLine" displayName="Show Line" type="Bool">true</Property>
				<Property name="hasShadow" displayName="Show Shadow" type="Bool">false</Property>
				<Property name="withMargin" displayName="Line has margin?" type="Bool">false</Property>
			</PropertyGroup>
			<PropertyGroup name="Appearance">
                <Property name="lineColor" displayName="Line Color" type="Color">#e8e8e8</Property>
				<Property name="customStyle3" displayName="Separator Line CSS" type="PlainText"><![CDATA[]]></Property>
			</PropertyGroup>
		</Properties>

		<Behaviors>
			<For ref="line">
				<Attr>
					<Arg>"style"</Arg>
					<Arg>
					<!-- Drawing of close "x" button -->
										   ($hasLine.value ? "display: block;" : "display:none;") +
										   "position:absolute;" +
										   "bottom:18px;" +
										   "width: 800px;" +
										   ($withMargin.value ? "width: 770px; margin-left:15px;" : " ") +
										   "height: 1px;" +
										   "background-color: " + $lineColor.toRGBString() + ";" +
										   $customStyle3.value
					</Arg>
				</Attr>
			</For>
			<For ref="ellipseShadow">
				<Visibility>$hasShadow</Visibility>
			</For>
			<For ref="container">
				<Box>new Dimension(800, 36);</Box>
			</For>
		</Behaviors>

		<p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
			<!--Gaussian Blur filter for half-ellipse shadow-->
			<filter id="gausBlur" x="0" y="0.5">
				<feGaussianBlur stdDeviation="5.45" />
			</filter>
			<foreignObject id="container" width="800" height="36" x="0" y="0">
				<div xmlns="http://www.w3.org/1999/xhtml" id="line"></div>
			</foreignObject>

			<!--Creates an half-ellipse bottom shadow-->
			<ellipse id="ellipseShadow" cx="400" cy="18" rx="380" ry="10" style="fill:#000000;fill-opacity:0.04;stroke:none" filter="url(#gausBlur)" />
		</p:Content>
	</Shape>

	<Shape id="lineDividerX" displayName="Line Divider X" icon="Icons/sectionHeader.png">
		<Properties>
			<PropertyGroup name="Properties">
				<Property name="box" type="Dimension" p:lockHeight="true">800,36</Property>
				<Property name="hasLine" displayName="Show Line" type="Bool">true</Property>
				<Property name="hasShadow" displayName="Show Shadow" type="Bool">false</Property>
				<Property name="withMargin" displayName="Line has margin?" type="Bool">false</Property>
			</PropertyGroup>
			<PropertyGroup name="Line Style">
                <Property name="lineColor" displayName="Line Color" type="Color">#e8e8e8</Property>
				<Property name="customStyle3" displayName="Separator Line CSS" type="PlainText"><![CDATA[]]></Property>
			</PropertyGroup>
			<PropertyGroup name="Shadow Style">
				<Property name="shadowColor" displayName="Shadow Color" type="Color">#0000000a</Property>
			</PropertyGroup>
		</Properties>

		<Behaviors>
			<For ref="line">
				<Attr>
					<Arg>"style"</Arg>
					<Arg>
					<!-- Drawing of close "x" button -->
										   ($hasLine.value ? "display: block;" : "display:none;") +
										   "position:absolute;" +
										   "bottom:18px;" +
										   "width: " + $box.w + "px;" +
										   ($withMargin.value ? "width: " + ($box.w - 30) + "px; margin-left:15px;" : " ") +
										   "height: 1px;" +
										   "background-color: " + $lineColor.toRGBString() + ";" +
										   $customStyle3.value
					</Arg>
				</Attr>
			</For>
			<For ref="ellipseShadow">
				<Attr>
					<Arg>"cx"</Arg>
					<Arg>$box.w / 2</Arg>
				</Attr>
				<Attr>
					<Arg>"cy"</Arg>
					<Arg>$box.h / 2</Arg>
				</Attr>
				<Attr>
					<Arg>"rx"</Arg>
					<Arg>($box.w/2) - 20</Arg>
				</Attr>
				<Attr>
					<Arg>"ry"</Arg>
					<Arg>10</Arg>
				</Attr>
				<!--Passing opacity value so can adjust from property dialog-->
				<Fill>$shadowColor</Fill>
				<Visibility>$hasShadow</Visibility>
			</For>
			<For ref="container">
				<Box>$box</Box>
			</For>
		</Behaviors>

		<Actions>
			<Action id="resizeToOriginal" displayName="Set to original size">
				<Impl>
					<![CDATA[
						this.setProperty("box", new Dimension(800, 23));
					]]>
				</Impl>
			</Action>
		</Actions>

		<p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
			<!--Gaussian Blur filter for half-ellipse shadow-->
			<filter id="gausBlur" x="0" y="0.5">
				<feGaussianBlur stdDeviation="5.45" />
			</filter>
			<foreignObject id="container" x="0" y="0">
				<div xmlns="http://www.w3.org/1999/xhtml" id="line"></div>
			</foreignObject>

			<!--Creates an half-ellipse bottom shadow-->
			<ellipse id="ellipseShadow" style="stroke:none" filter="url(#gausBlur)" />
		</p:Content>
	</Shape>

	<Shape id="BtnBlue" displayName="Blue Button" icon="Icons/btnBlue.png">
		<Properties>
			<PropertyGroup>
				<Property name="box" type="Dimension" p:lockHeight="true">95,30</Property>
			</PropertyGroup>
			<PropertyGroup name="Properties">
				<Property name="textContent" displayName="Content" type="PlainText" p:editInfo="({targetName: 'container', bound: new Bound(5, ($box.h/2)-15, $box.w-10, 30), font: $textFont, align: new Alignment(1, 1), multi: false})">Button</Property>
				<Property system="true" name="state" displayName="State" type="Enum" p:enumValues="['normal|Normal', 'hover|Mouse Over', 'clicked|Clicked']">normal</Property>
				<Property name="position" displayName="Button Group Position" type="Enum" p:enumValues="['none|None','left|Left','middle|Middle','right|Right']">none</Property>
				<Property name="disabled" displayName="Disabled?" type="Bool">false</Property>
			</PropertyGroup>
			<PropertyGroup name="Text">
				<Property name="textFont" displayName="Font" type="Font"><E>$$defaultFont14</E></Property>
				<Property name="textColor" displayName="Color" type="Color">#FFFFFF</Property>
			</PropertyGroup>
			<PropertyGroup name="Additional CSS">
				<Property name="customStyle1" displayName="Background" type="PlainText"><![CDATA[]]></Property>
				<Property name="customStyle2" displayName="Text" type="PlainText"><![CDATA[]]></Property>
				<Property name="customStyleHidden" type="PlainText"><![CDATA[]]></Property>
			</PropertyGroup>
		</Properties>
		<Behaviors>
            <For ref="divObject">
				<InnerText>$textContent.value</InnerText>
                <Attr>
                    <Arg>"style"</Arg>
                    <Arg>
						<!-- Standard CSS for mapping built-in palettes to CSS -->
						"font-family: " + $textFont.family + ";" +
						"font-size: " + $textFont.size + ";" +
						"font-style: " + $textFont.style + ";" +
						"font-weight: " + $textFont.weight + ";" +
						"text-decoration: " + $textFont.decor + ";" +
						"color: " + $textColor.toRGBString() + ";" +
						
						<!-- Set your CSS behaviours -->
						"display: inline-block;" + 
						"border-width: 1px;" +
						"border-style: solid;" +
						"border-radius: 5px;" +

						"height: " + ($box.h-2) + "px;" +
						"width: " + ($box.w-2) + "px;" +
						"line-height: " + ($box.h-4) + "px;" +

						"text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.16);" +
						"text-align: center;" +
						"background-image: -moz-linear-gradient(top, #00a5ff, #0084cc);" +
						"border-color:#0f6597;" +
						"color:#ffffff;" +
						"text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.16);" +
						"box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.25);" +						
						($state.value == "clicked" ? "background-image: none; background-color: #007DC2; box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.15);" : "") +
						($state.value == "hover" ? "background-image: none; background-color: #007DC2;" : "") +
						($disabled.value ? "background-image: none; background-color: #007DC2; box-shadow: none; opacity: 0.65;" : "") +
						$customStyleHidden.value +
						$customStyle1.value + 
						$customStyle2.value
					</Arg>
                </Attr>
				<CustomStyle>
					<Arg>"border-top-left-radius"</Arg>
					<Arg>$position.value == "none" || $position.value == "left" ? "5px" : "0px"</Arg>
				</CustomStyle>
				<CustomStyle>
					<Arg>"border-top-right-radius"</Arg>
					<Arg>$position.value == "none" || $position.value == "right" ? "5px" : "0px"</Arg>
				</CustomStyle>
				<CustomStyle>
					<Arg>"border-bottom-right-radius"</Arg>
					<Arg>$position.value == "none" || $position.value == "right" ? "5px" : "0px"</Arg>
				</CustomStyle>
				<CustomStyle>
					<Arg>"border-bottom-left-radius"</Arg>
					<Arg>$position.value == "none" || $position.value == "left" ? "5px" : "0px"</Arg>
				</CustomStyle>
            </For>
            <For ref="container">
            	<Box>$box</Box>
            </For>
        </Behaviors>
		<Actions>
			<Action id="resizeToMicro" displayName="Resize to Small">
				<Impl>
					<![CDATA[
						var box = this.getProperty("box");
						this.setProperty("box", new Dimension(65, 23));
						this.setProperty("textFont", "Segoe UI|normal|normal|11px");
					]]>
				</Impl>
			</Action>
			<Action id="resizeToStandard" displayName="Resize to Standard">
				<Impl>
					<![CDATA[
						var box = this.getProperty("box");
						this.setProperty("box", new Dimension(95, 30));
						this.setProperty("textFont", "Segoe UI|normal|normal|14px");
					]]>
				</Impl>
			</Action>
			<Action id="resizeToLarge" displayName="Resize to Large">
				<Impl>
					<![CDATA[
						var box = this.getProperty("box");
						this.setProperty("box", new Dimension(110, 41));
						this.setProperty("textFont", "Segoe UI|normal|normal|16px");
					]]>
				</Impl>
			</Action>
		</Actions>
		<p:Content xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns="http://www.w3.org/2000/svg">
            <foreignObject id="container">
                <div xmlns="http://www.w3.org/1999/xhtml" id="divObject"></div>
            </foreignObject>
        </p:Content>
	</Shape>

